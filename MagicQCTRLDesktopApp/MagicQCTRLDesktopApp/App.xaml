<Application x:Class="MagicQCTRLDesktopApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MagicQCTRLDesktopApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ThemesV2/SoftDark.xaml"/>
                <ResourceDictionary Source="ThemesV2/ControlColours.xaml"/>
                <ResourceDictionary Source="ThemesV2/Controls.xaml"/>
                <ResourceDictionary Source="ThemesV2/CustomControls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <ImageSource x:Key="IconImage">/Resources/IconFull.ico</ImageSource>
        </ResourceDictionary>
    </Application.Resources>
</Application>
